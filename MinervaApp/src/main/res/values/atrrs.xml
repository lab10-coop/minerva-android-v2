<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PassiveVideoView">
        <attr name="fallbackImage" format="integer" />
        <attr name="loadingImage" format="integer" />
        <attr name="video" format="integer" />
    </declare-styleable>

    <declare-styleable name="PredefinedNetworkView">
        <attr name="icon" format="reference" />
        <attr name="name" format="reference" />
    </declare-styleable>

    <declare-styleable name="RecyclableViewMoreTextView">
        <attr format="integer" name="visibleLines"/>
        <attr format="boolean" name="isExpanded"/>
        <attr format="integer" name="durationMultiplier"/>
        <attr format="integer" name="maxDuration"/>
        <attr format="color" name="foregroundColor"/>
        <attr format="string" name="ellipsizeText"/>
        <attr format="boolean" name="isUnderlined"/>
        <attr format="color" name="ellipsizeTextColor"/>
    </declare-styleable>
</resources>